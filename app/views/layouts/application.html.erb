<!DOCTYPE html>
<html>
  <head>
    <title>LittleEtsyShop</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <%flash.each do |type, msg|%>
  <div>
    <%=msg%>
  </div>
  <%end%>
    <%= yield %>
    <div id="footer">
     <%= render partial: 'layouts/footer', locals: {name: @repository_name, group: @group_usernames, contributions: @contributor_commits, pull_requests: @pull_requests} %>
    </div>
  </body>
</html>
